#initialize values
f = open('dictionary.lst')
contents = f.read()
contents = contents.lower()
content_list = contents.split()
inputString = input().lower()
ciphertext = inputString.split()

longest = 0  
for i in ciphertext:
    if len(i) > longest:
        longest = len(i)
        longestWord = i

def isIsomorphic(str1, str2):
    len1 = len(str1)
    len2 = len(str2)
    if len1 != len2:
        return(False)
    dict1 = {k:[False] for k in set(list(str1))}
    dict2 = {}
    for i in range(len(str1)):
        if dict1[str1[i]] == [False]:
      #a false value matching a previously mapped value
            if str2[i] in list(dict2):
                return(False)
            else:
                dict1[str1[i]] = str2[i]
                dict2[str2[i]] = str1[i]
    #check if key matches this non false value to catch duplicate in str1
        elif dict1[str1[i]] != str2[i]:
            return(False)
    return(True)

#now we want to start by finding an isomorphic match on the longest word
def decipher(cipherword, crib, ctext=ciphertext, wordList=content_list, length=longest):
        revKey = dict(zip(set(crib),set(cipherword)))
        key = dict(zip(list(cipherword),list(crib)))
        n=longest
        while n > 1 and len(list(key)) < 26:
            for cipherword in ctext:
                if len(cipherword) == n:
                    for word in wordList:
                        if isIsomorphic(cipherword,word) is True:
                            for numletter in range(len(cipherword)):
                                if cipherword[numletter] not in list(key):
                                    key[cipherword[numletter]]=word[numletter]
                                    revKey[word[numletter]]=cipherword[numletter]
            n=n-1
        return(key)


        #the first thing we need to do is build the key

for i in content_list:
    if len(i) == longest:
        if isIsomorphic(i, longestWord) is True:   
            
            codebreak = decipher(longestWord,i)
            #begin deciphering here I guess.
            plaintext = ''
codebreak[" "]=" "
for letter in inputString:
    plaintext=plaintext+str(codebreak[letter])
print(plaintext) 
